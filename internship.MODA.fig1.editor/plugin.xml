         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated fig1 -->
      <editor
            id="internship.moda.fig1.presentation.Fig1EditorID"
            name="%_UI_Fig1Editor_label"
            icon="icons/full/obj16/Fig1ModelFile.gif"
            extensions="fig1"
            class="internship.moda.fig1.presentation.Fig1Editor"
            contributorClass="internship.moda.fig1.presentation.Fig1ActionBarContributor">
      </editor>
   </extension>

</plugin>
