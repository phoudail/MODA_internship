<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated fig1 -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="internship.moda.fig1.presentation.Fig1ModelWizardID"
            name="%_UI_Fig1ModelWizard_label"
            class="internship.moda.fig1.presentation.Fig1ModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/Fig1ModelFile.gif">
